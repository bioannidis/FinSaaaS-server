<?xml version="1.0" encoding="UTF-8"?>
<web-app id="WebApp_ID" version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <listener>
        <listener-class>
		org.saaas.server.ApiKeyInitializer
    </listener-class>
    </listener>
    <servlet>
        <servlet-name>RegisterServlet</servlet-name>
        <servlet-class>
      org.saaas.server.RegisterServlet
    </servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet>
        <servlet-name>GetAllContributors</servlet-name>
        <servlet-class>org.saaas.server.GetAllContributors</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SetReachabilityServlet</servlet-name>
        <servlet-class>org.saaas.server.SetReachabilityServlet</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet>
        <servlet-name>ResetTimerServlet</servlet-name>
        <servlet-class>org.saaas.server.ResetTimerServlet</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet>
        <servlet-name>DownloadServlet</servlet-name>
        <servlet-class>org.saaas.server.DownloadServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UnregisterServlet</servlet-name>
        <servlet-class>
      org.saaas.server.UnregisterServlet
    </servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet>
        <servlet-name>SendAllMessagesServlet</servlet-name>
        <servlet-class>
      org.saaas.server.SendAllMessagesServlet
    </servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet>
        <servlet-name>SendAllSubscriptorMessagesServlet</servlet-name>
        <servlet-class>
      org.saaas.server.SendAllSubscriptorMessagesServlet
    </servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet>
        <servlet-name>SendAllContributorMessagesServlet</servlet-name>
        <servlet-class>
      org.saaas.server.SendAllContributorMessagesServlet
    </servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet>
        <servlet-name>SendAllDeveloperMessagesServlet</servlet-name>
        <servlet-class>
      org.saaas.server.SendAllDeveloperMessagesServlet
    </servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet>
        <servlet-name>GetContributorsServlet</servlet-name>
        <servlet-class>
      org.saaas.server.GetContributorsServlet
    </servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet>
        <servlet-name>HomeServlet</servlet-name>
        <servlet-class>
      org.saaas.server.HomeServlet
    </servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet>
        <servlet-name>UploadServlet</servlet-name>
        <servlet-class>org.saaas.server.UploadServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RegisterServlet</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UnregisterServlet</servlet-name>
        <url-pattern>/unregister</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SendAllMessagesServlet</servlet-name>
        <url-pattern>/sendAll</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SendAllSubscriptorMessagesServlet</servlet-name>
        <url-pattern>/sendAllSubscriptor</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SendAllContributorMessagesServlet</servlet-name>
        <url-pattern>/sendAllContributor</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SendAllDeveloperMessagesServlet</servlet-name>
        <url-pattern>/sendAllDeveloper</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetContributorsServlet</servlet-name>
        <url-pattern>/getcontributors</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>HomeServlet</servlet-name>
        <url-pattern>/home</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SetReachabilityServlet</servlet-name>
        <url-pattern>/setreachability</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ResetTimerServlet</servlet-name>
        <url-pattern>/resettimer</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetAllContributors</servlet-name>
        <url-pattern>/GetAllContributors</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DownloadServlet</servlet-name>
        <url-pattern>/download</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UploadServlet</servlet-name>
        <url-pattern>/upload</url-pattern>
    </servlet-mapping>
</web-app>
